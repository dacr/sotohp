{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "outDir": "assets/js",
    "rootDir": "src",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmitOnError": false,
    "sourceMap": false
  },
  "include": ["src/**/*.ts"]
}